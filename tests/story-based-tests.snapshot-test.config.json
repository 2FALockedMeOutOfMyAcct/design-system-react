{
	"rootDir": "../",
	"testRegex": "/(components|tests)/.*\\.snapshot-test\\.jsx?$",
	"testEnvironment": "node",
	"testPathIgnorePatterns": [
		"/node_modules/",
		"/preset/",
		"/.tmp-amd/",
		"/.tmp-commonjs/",
		"/.tmp-es/",
		"/.tmp-npm/"
	],
	"transform": {
		"^.+\\.jsx?$": "babel-jest",
		"^.+\\.svg$": "jest-svg-transformer"
	},
	"transformIgnorePatterns": [
		"/node_modules/",
		"/preset/",
		"/.tmp-amd/",
		"/.tmp-commonjs/",
		"/.tmp-es/",
		"/.tmp-npm/"
	]
}
